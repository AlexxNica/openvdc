{
  "search.exclude": {
    "vendor/**": true,
    "vendor/vendor.json": false
  },
  "protoc": {
    "options": [
      "--proto_path=${workspaceRoot}/proto",
      "--proto_path=${env.GOPATH}/src"
    ]
  },
  "json.schemas": [
    {
      "fileMatch": [
        "/templates/**/*.json"
      ],
      "url": "./schema/v1.json"
    }
  ]
}
